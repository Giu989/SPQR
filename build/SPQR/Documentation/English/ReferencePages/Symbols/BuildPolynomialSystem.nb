Cell[BoxData[{
 RowBox[{
  RowBox[{"variables", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"targets", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"a", "*", 
       RowBox[{"x", "^", "5"}], "*", 
       RowBox[{"y", "^", "5"}], "*", 
       RowBox[{"y", "^", "2"}]}], "+", "3", "+", 
      RowBox[{"x", "*", 
       RowBox[{"y", "^", "2"}]}], "+", 
      RowBox[{"b", "^", "2"}]}], ",", 
     RowBox[{
      RowBox[{"x", "*", "y"}], "+", "s", "+", "3"}], ",", 
     RowBox[{"x", "^", "3"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ideal", " ", "=", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "*", 
        RowBox[{"y", "^", "2"}], "*", 
        RowBox[{"x", "^", "2"}]}], "-", 
       RowBox[{"2", 
        RowBox[{"x", "^", "2"}]}], "+", "3"}], ",", 
      RowBox[{
